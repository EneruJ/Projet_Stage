{# templates/home/index.html.twig #}
{% extends "base_connected.html.twig" %}

{% block title %}Accueil | {{ parent() }}{% endblock %}

{% block content %}


    <main role="main" class="container" >


            <a href="{{ path('filtre', {info : "Blabla"}) }}"> Titre = Blabla</a>
            <a href="{{ path('filtre', {info : "Enerou"}) }}">idUser = Enerou</a>

        <HR align=center size=8 width="50%">
        {% for document in documents %}
        <div class="container">

            <div class="well">
                <div class="media">
                    <a class="pull-left" href="#">
                        <img src="data:image/png;base64," alt="image"/>
                    </a>
                    <div class="media-body">
                        <h4 class="media-heading">{{ document.titre }}</h4>
                        <p class="text-right">{{ document.idUser }}</p>
                        <p>{{  document.Resume | slice(0,100) }}</p>
                    </div>
                    <div class="d-flex justify-content-between align-items-center">
                        <div class="btn-group">
                            <button type="button" class="btn btn-sm btn-outline-secondary"><a href="{{ path('show_pdf', {"name" : document.docName}) }}" style="text-decoration: none; color: black;">Download</a></button>
                            <button type="button" class="btn btn-sm btn-outline-secondary"><a href="{{ path('filtre') }}" style="text-decoration: none; color: black;">Delete</a></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {% endfor %}
    </main>
{% endblock %}

